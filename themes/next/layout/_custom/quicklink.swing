{% if theme.quicklink.enabled %}
<script src="/js/src/quicklink.umd.js"></script>
<script>
window.addEventListener('load', () =>{
   quicklink({
     ignores: [ /#(.+)/ , /\.txt/, /\.sh/,
     /\.zip/, /\.rar/, /\.7z/, /\.tar/,
     /\.png/, /\.jpg/, /\.jpeg/, /\.gif/ ]
   });
});
</script>
{% endif %}